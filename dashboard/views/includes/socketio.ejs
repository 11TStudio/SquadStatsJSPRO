<script src="https://cdn.socket.io/4.1.2/socket.io.js" crossorigin="anonymous"></script>
<script>
    const url = "<%= config.socketIOURL %>";
    const urlNorthflank = "<%= config.socketIOURL_Nortflank %>"
    let socket = {};

    if(urlNorthflank){
        socket = io(
            urlNorthflank,
            {
                transports: ["websocket", "polling"],
                withCredentials: true,
                auth: {
                    token: "<%= userDiscord.apiToken %>",
                },
            }
        );
    }

    // check if url is localhost
    if (!urlNorthflank && url && url.includes("localhost")) {
        socket = io(
            "<%= config.socketIOURL %>:<%=config.socketIO.localPort%>",
            {
                transports: ["websocket", "polling"],
                withCredentials: true,
                auth: {
                    token: "<%= userDiscord.apiToken %>",
                },
            }
        )
    }

    
    
    
</script>