// ----- Groups ----- //
<% if(clan) {%> 
Group=Whitelist:reserve
<% for(member in members){%> 
<% if(members[member].whitelist.byClan === true) {%> 
Admin=<%- members[member].steam.steamid %>:Whitelist // <%- members[member].name %>  
<% } %> 
<% } %> 
<% }else{%>
<% for(role in roles) { %>
Group=<%= role %>:<%= roles[role].permissions %>
<% }; %>

// ----- Users ----- // 
<% for(member in memberData) { %>
Admin=<%= member %>:<%= memberData[member].role %> // <%= memberData[member].description %>
<% }; %>

<% } %> 
//</pre>