// ----- Groups ----- //
Group=Whitelist:reserve
<% for(role in roles) { %>
Group=<%= role %>:<%= roles[role].permissions %>
<% }; %>
<% for(member in autoClans){%> 
<% if(autoClans[member].whitelist.byClan === true) {%> 
Admin=<%- autoClans[member].steam.steamid %>:Whitelist // <%- autoClans[member].name %>  
<%}%><%}%> 
// ----- Users ----- // 
<% for(member in memberData) { %>
Admin=<%= member %>:<%= memberData[member].role %> // <%= memberData[member].description %>
<% }; %>
// ----- Clans ----- //
<% for(clan in manualClans){%> 
<% for(i in manualClans[clan].manualWhitelistedUsers[0]){%> 
Admin=<%- manualClans[clan].manualWhitelistedUsers[0].steamID %>:Whitelist // <%- manualClans[clan].manualWhitelistedUsers[0].name %>
<% }; %>
<% }; %>
