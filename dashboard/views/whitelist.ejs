// ----- Groups ----- //
<% for(role in roles) { %>
    Group=<%= role %>:<%= roles[role].permissions %>
<% }; %>

// ----- Users ----- // 
<% for(member in memberData) { %>
    Admin=<%= member %>:<%= memberData[member].role %> // <%= memberData[member].description %>
<% }; %>